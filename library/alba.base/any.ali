use boolean end

deferred class ANY end

G: ANY


(=) (a,b:G): BOOLEAN    deferred end

(/=) (a,b:G): BOOLEAN -> not (a = b)

all(a:G) ensure a = a deferred end

all(a:G) ensure a /= a ==> false end

immutable class boolean.BOOLEAN
inherit         ANY end
