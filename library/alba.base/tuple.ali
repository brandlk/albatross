use
    predicate
end


A: ANY
B: ANY

class
    TUPLE[A,B]
create
    tuple(first:A, second:B)
end

first (t:(A,B)): A
        -- The first element of the tuple 't'.
    -> inspect
           t
       case (a,_) then
           a

second (t:(A,B)): B
        -- The second element of the tuple 't'.
    -> inspect
           t
       case (_,b) then
           b
